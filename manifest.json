{
    "name":"Color picker",
    "description":"A Color picker extension",
    "version":"1.0",
    "manifest_version":3,
    "background":{
        "ServiceWorker":"background.js"
    },
    "permissions":[
        "storage",
        "activeTab",
        "scripting"
    ],
    "options_page":"options.html",
    "action":{
        "default_popup":"popup.html"
    },
    "icons":{
        "16":"/images/colour16.png",
        "32":"/images/colour32.png",
        "48":"/images/colour48.png"
    }
}